{
  "name": "@vuedoc/parser",
  "version": "4.0.0-beta3",
  "description": "Generate a JSON documentation for a Vue file",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./lib/entity/MethodEntry.js": "./lib/entity/MethodEntry.js"
    "./lib/Enum.js": "./lib/Enum.js",
    "./lib/entity/ComputedEntry.js": "./lib/entity/ComputedEntry.js",
    "./lib/entity/DataEntry.js": "./lib/entity/DataEntry.js",
    "./lib/entity/DescriptionEntry.js": "./lib/entity/DescriptionEntry.js",
    "./lib/entity/EventEntry.js": "./lib/entity/EventEntry.js",
    "./lib/entity/InheritAttrsEntry.js": "./lib/entity/InheritAttrsEntry.js",
    "./lib/entity/Keyword.js": "./lib/entity/Keyword.js",
    "./lib/entity/KeywordsEntry.js": "./lib/entity/KeywordsEntry.js",
    "./lib/entity/MethodEntry.js": "./lib/entity/MethodEntry.js",
    "./lib/entity/ModelEntry.js": "./lib/entity/ModelEntry.js",
    "./lib/entity/NameEntry.js": "./lib/entity/NameEntry.js",
    "./lib/entity/PropEntry.js": "./lib/entity/PropEntry.js",
    "./lib/entity/RestValue.js": "./lib/entity/RestValue.js",
    "./lib/entity/SlotEntry.js": "./lib/entity/SlotEntry.js",
    "./lib/entity/Value.js": "./lib/entity/Value.js",
    "./lib/entity/Comment.js": "./lib/entity/Comment.js",
    "./lib/JSDoc.js": "./lib/JSDoc.js",
    "./loaders/html.js": "./loaders/html.js",
    "./loaders/javascript.js": "./loaders/javascript.js",
    "./loaders/pug.js": "./loaders/pug.js",
    "./loaders/typescript.js": "./loaders/typescript.js",
    "./loaders/vue.js": "./loaders/vue.js"
  },
  "typings": "index.d.ts",
  "scripts": {
    "preversion": "yarn test",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "lint": "eslint .",
    "gimtoc": "gimtoc -f README.md -s 'Table of Contents' -o README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/vuedoc/parser.git"
